@charset "utf-8";

body { min-height: 480px; line-height: 1.5em; -webkit-font-smoothing: antialiased; font-family: roboto, sans-serif; font-size: 15px; color: rgb(89, 88, 88); visibility: visible !important; }

:focus, a:focus { outline: none; }

input[type="text"], input[type="password"], input[type="email"], input[type="tel"], textarea, select, .select2-container--default .select2-selection--single { font-size: 15px; padding: 10px 17px; color: rgb(89, 88, 88); border: 1px solid rgb(218, 218, 218); border-radius: 0px; background: rgb(255, 255, 255); margin: 0px 0px 20px; max-width: 100%; outline: none; }

.select2-container--default .select2-selection--single { margin: 0px; min-height: 44px; }

.select2-container--default .select2-selection--single .select2-selection__rendered { line-height: 1.5em; padding: 0px; }

.select2-container--default .select2-selection--single .select2-selection__arrow { right: 10px; top: 8px; }

input[type="text"], input[type="password"], input[type="email"], input[type="tel"], textarea { }

p { margin: 0px 0px 21px; }

hr { border-top: 6px solid rgb(51, 51, 51); margin-top: 30px; margin-bottom: 30px; }

hr.light { border-top-color: rgb(221, 221, 221); }

hr:first-child { margin-top: 0px; }

hr.hr-thin-bottom { margin-bottom: 15px; }

hr.hr-thin-top { margin-top: 15px; }

h1, h2, h3, h4, h5, h6, .comment-reply-title, .widget_shopping_cart_content .buttons, .widget_popular_post .nav-popular-post li a { margin-bottom: 20px; overflow-wrap: break-word; font-family: poppins, sans-serif; font-weight: 600; color: rgb(51, 51, 51); line-height: 1.4em; }

h1 a, h2 a, h3 a, h4 a, h5 a, h6 a { color: rgb(51, 51, 51); }

h1 [class^="tm-"], h1 > [class*=" tm-"], h2 [class^="tm-"], h2 > [class*=" tm-"], h3 [class^="tm-"], h3 > [class*=" tm-"], h4 [class^="tm-"], h4 > [class*=" tm-"], h5 [class^="tm-"], h5 > [class*=" tm-"], h6 [class^="tm-"], h6 > [class*=" tm-"] { font-size: inherit; }

h1 + h2, h1 + h3, h1 + h4, h1 + h5, h1 + h6 { margin-top: 0px; }

h2 + h3, h2 + h4, h2 + h5, h2 + h6 { margin-top: 0px; }

h3 + h4, h3 + h5, h3 + h6 { margin-top: 0px; }

img { max-width: 100%; height: auto; }

blockquote { font-family: georgia; margin: 27px 0px; font-style: italic; background-image: url("../../images/quote.gif"); background-repeat: no-repeat; background-color: rgb(245, 246, 247); background-size: 23px; background-position: 24px 30px; padding: 25px 25px 25px 70px; }

blockquote p { font-size: 1.2em; line-height: 1.5; color: rgb(51, 51, 51); }

blockquote > * { margin-bottom: 0px; }

pre { margin: 1.5em 0px; border-style: dashed; line-height: 2em; border-radius: 0px; }

abbr[title] { border: 0px !important; }

em { font-style: italic; font-weight: inherit !important; }

a { color: rgb(89, 88, 88); }

a:hover { color: inherit; }

table { }

table thead { background-color: rgb(245, 245, 245); }

table th { padding: 25px 10px; font-family: poppins, sans-serif; font-weight: 600; vertical-align: top; }

table td { padding: 5px 10px; }

table tr + tr { border-top: 1px solid rgb(231, 231, 231); }

mark { padding: 1px 5px 2px; background-color: transparent; border: 1px solid rgb(229, 229, 229); border-radius: 3px; }

.site-outline { display: none !important; }

.clearfix::before, .clearfix::after { content: ""; display: table; }

.clearfix::after { clear: both; }

.clearfix { zoom: 1; }

.both { clear: both; }

.alignleft { float: left; }

.alignright { float: right; }

.aligncenter { text-align: center; margin-left: auto; margin-right: auto; }

img.aligncenter { margin-left: auto; margin-right: auto; display: block; }

.wp-caption { position: relative; margin-bottom: 13px; display: inline-block; max-width: 100%; }

.wp-caption img { transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s; margin-bottom: 0px !important; }

.wp-caption-text { position: absolute; left: 0px; right: 0px; bottom: 0px; padding: 30px 14px 8px; font-size: 0.9em; color: rgb(255, 255, 255); text-shadow: rgb(0, 0, 0) 1px 1px 2px; background-image: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.3)); margin-bottom: 0px !important; }

.wp-caption > a { display: inline-block; width: 100%; height: 100%; background: rgb(0, 0, 0); transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s; }

.wp-caption:hover > a img { opacity: 0.9; }

.text-content img { margin-bottom: 13px; }

.text-content a { }

.text-content ul, .text-content ol { margin-bottom: 21px; }

img.alignleft, .wp-caption.alignleft { margin-right: 23px; }

img.alignright, .wp-caption.alignright { margin-left: 23px; }

.wp-caption.aligncenter { display: table; }

.wp-caption.aligncenter figure { position: relative; display: inline-block; }

.bypostauthor { }

.gallery-caption { }

.maha-wp-gallery { clear: both; margin-bottom: 20px; }

.maha-wp-gallery figure { float: left; padding: 2px; position: relative; }

.maha-wp-gallery .g-size-1 { width: 100%; }

.maha-wp-gallery .g-size-2 { width: 50%; }

.maha-wp-gallery .g-size-2 img { width: 100%; }

.maha-wp-gallery .g-size-3 { width: 33.3333%; }

.maha-wp-gallery .g-size-4 { width: 25%; }

.maha-wp-gallery figure a { opacity: 0.9; cursor: pointer; border: 0px !important; }

.maha-wp-gallery figure .caption-wrap { position: relative; }

.maha-wp-gallery figure img { margin: 0px; padding: 0px; }

.screen-reader-text { clip: rect(1px, 1px, 1px, 1px); height: 1px; overflow: hidden; width: 1px; position: absolute !important; overflow-wrap: normal !important; }

.hidden { display: none; }

.text-center { text-align: center; }

.text-right { text-align: right; }

.vp-970 { display: none; }

.vp-750 { display: none; }

.vp-fluid { display: none; }

.vp-1170 { display: block; }

.vp-320 { display: none; }

.page-sidebar .container .col-sm-4 { position: static; }

#off-canvas-body { background: rgb(255, 255, 255); position: relative; left: 0px; height: auto; transition: transform 0.5s ease 0s; }

.mobile-bar { width: 300px; top: 0px; left: 0px; position: absolute; visibility: hidden; transition: all 0.5s ease 0s; transform: translateX(-50%); background: rgb(33, 33, 33); }

.mobile-bar #close-mobile-bar { text-align: center; color: rgb(234, 234, 234); padding: 0px; cursor: pointer; font-size: 26px; line-height: 46px; transition: all 0.2s ease-in-out 0s; }

.mobile-bar #close-mobile-bar:hover { color: rgb(49, 187, 137); }

.mobile-bar ul { margin: 0px; padding: 0px; }

.mobile-bar ul ul { display: none; margin-left: 21px; }

.mobile-bar ul li { position: relative; display: block; width: 100%; font-family: poppins, sans-serif; font-weight: 600; }

.mobile-bar ul li a { color: rgb(234, 234, 234); font-size: 14px; text-transform: uppercase; display: block; position: relative; line-height: 1.4em; text-decoration: none; margin: 0px; padding: 14px 19px; transition: all 0.2s ease-in-out 0s; }

.mobile-bar ul li a:hover, .mobile-bar ul li.current-menu-item > a { color: rgb(49, 187, 137); }

.mobile-bar ul li .navmob-sub-menu { cursor: pointer; right: 0px; position: absolute; font-size: 20px; z-index: 999; padding: 12px 12px 11px 10px; transition: all 0.2s ease-in-out 0s; }

.mobile-bar ul li .navmob-sub-menu:hover, .mobile-bar ul li .navmob-sub-menu .tm-up-open-mini { color: rgb(234, 234, 234); }

#search-mobile-bar input { width: 100%; margin-bottom: 0px; border: 0px; }

#search-mobile-bar .search-button { position: absolute; background: 0px 0px; border: none; right: 0px; padding: 7px 11px; }

#body-maha { overflow-x: hidden; background: rgb(33, 33, 33); }

#body-maha.st-menu-open #off-canvas-body { overflow: hidden; transform: translateX(300px); }

#body-maha.st-menu-open .mobile-bar { visibility: visible; transition: transform 0.5s ease 0s; transform: translateX(0px); }

#top-bar-sticky { background: rgb(33, 33, 33); color: rgb(234, 234, 234); }

#top-bar-sticky a { color: rgb(234, 234, 234); }

.ul-nav ul { margin-bottom: 0px; padding: 0px; }

.ul-nav ul li { display: inline-block; font-family: poppins, sans-serif; font-weight: 600; text-transform: uppercase; font-size: 11px; }

.ul-nav ul#menu-top-menu > li > a { margin: 2px 0px; }

.ul-nav ul li a { display: block; padding: 2px 10px 0px; line-height: 38px; text-decoration: none; transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s; }

.ul-nav ul li ul { padding-bottom: 6px; visibility: hidden; position: absolute; width: 180px; z-index: 10000; background: rgb(33, 33, 33); transition: all 0.2s ease-in-out 0s; opacity: 0; }

.ul-nav ul li:hover ul { opacity: 1; visibility: visible; }

.ul-nav ul li ul li { display: block; border: 0px; }

.ul-nav ul li ul li a { font-size: 13px; line-height: 36px; padding: 0px 15px; border: 0px; }

#top-bar-sticky a:hover, #top-bar-sticky .current-menu-item a { color: rgb(49, 187, 137); }

#top-right-nav { float: right; text-align: right; }

#top-right-nav > * { display: inline-block; margin-right: 15px; }

#top-right-nav > :last-child { margin-right: 0px; }

#top-right-nav .social-top ul { margin: 0px; padding: 0px; }

#top-right-nav .social-top ul li { display: inline-block; list-style: none; }

#top-right-nav .social-top ul li a { display: block; padding: 0px 10px; line-height: 38px; text-decoration: none; font-size: 15px; transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s; }

.top-bar .top-user { font-family: poppins, sans-serif; font-weight: 600; font-size: 11px; text-transform: uppercase; line-height: 38px; }

.top-bar .top-user a { text-decoration: none; transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s; }

.top-bar .top-user i { font-size: 16px; padding-right: 5px; }

#top-mobile-wrapper { display: none; }

#top-mobile-wrapper a { color: rgb(234, 234, 234); font-size: 24px; padding: 0px 11px; display: inline-block; cursor: pointer; line-height: 46px; margin-left: -10px; }

#thelogo { margin-left: 3px; }

.the-logo-center { float: none !important; }

#thelogosmall { float: left; height: 54px; }

#thelogosmall img { margin-top: 6px; position: absolute; opacity: 0; top: -20px; left: 30px; }

#thelogosmall.nav-main-affix img { transition: all 0.2s cubic-bezier(0.4, 0, 0.2, 1) 0s; opacity: 1; top: 0px; }

#main-ads .vp-970 { padding-top: 10px; }

.main-logo-ads-wrap { padding: 17px 0px 11px; }

.main-logo-ads .main-logo-center, .main-nav-center { text-align: center; }

.main-nav-center ul li ul { text-align: left; }

#main-nav-bar { position: relative; background: rgb(255, 255, 255); min-height: 4px; z-index: 9999; }

#main-nav-bar.on-stuck { position: fixed; top: 0px; z-index: 99999; width: 100%; opacity: 0.95; }

.admin-bar .sticky-wrapper.is-sticky .main-nav-bar { margin-top: 28px; }

.header-bar-sticky { position: fixed; top: 0px; z-index: 99999; left: 0px; right: 0px; }

.main-ul-nav ul, #main-nav-bar.on-stuck { padding: 0px; margin: 0px; }

#main-nav-bar.on-stuck, #main-nav-bar:not(.on-stuck) .main-ul-nav { box-shadow: rgb(51, 51, 51) 0px -4px 0px inset; }

.main-ul-nav ul li { display: inline-block; list-style: none; font-size: 13px; }

.main-ul-nav ul li a { font-weight: 600; font-size: 15px; color: rgb(51, 51, 51); line-height: 54px; text-transform: uppercase; display: block; padding: 0px 10px; text-decoration: none; transition: all 0.2s ease-in-out 0s; font-family: roboto, sans-serif !important; }

.main-ul-nav ul li:hover a { color: rgb(49, 187, 137); }

.main-ul-nav ul li .sub-ord-nav { margin-top: 19px; line-height: 1em; float: right; position: relative; width: 7px; right: -3px; }

.main-ul-nav ul li ul { display: none; }

.main-ul-nav ul li .nav-sub-wrap { position: absolute; left: 0px; width: 100%; z-index: 999; transition: opacity 0.3s ease-in 0s; opacity: 0; display: none; padding: 0px 25px; }

.main-ul-nav ul li .nav-sub-wrap .nsw { background-color: rgb(255, 255, 255); }

.main-ul-nav ul li .nav-sub-wrap:not(.ord-nav) .nsw { border-top: 0px; padding: 25px 10px; margin: 0px -10px; }

.main-ul-nav ul li .nav-sub-wrap:not(.ord-nav) .nsw.mm-full { padding: 25px; }

.main-ul-nav ul li .nav-sub-wrap .nsw-shadow { box-shadow: rgba(0, 0, 0, 0.2) 0px 2px 1px 0px; }

.main-ul-nav ul li .nav-sub-wrap ul { display: block; }

.main-ul-nav ul li:hover > .nav-sub-wrap { width: 100%; }

.main-ul-nav .nav-sub-wrap .mm-full { padding-left: 15px; padding-right: 15px; }

